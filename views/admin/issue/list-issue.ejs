<%- include("../partials/header") %>
        <!-- Content Header (Page header) -->
        <div class="content-header">
            <div class="container-fluid">
              <div class="row mb-2">
                <div class="col-sm-6">
                  <h1 class="m-0 text-dark">issue list history</h1>
                </div>
                <!-- /.col -->
                <div class="col-sm-6">
                  <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a href="#">Home</a></li>
                    <li class="breadcrumb-item active">issue list history</li>
                  </ol>
                </div>
                <!-- /.col -->
              </div>
              <!-- /.row -->
            </div>
            <!-- /.container-fluid -->
          </div>
          <!-- /.content-header -->
  
          <!-- Main content -->
          <section class="content">
            <div class="container-fluid">
              <!-- Small boxes (Stat box) -->
              <div class="row">
                <div class="card col-md-12">
                    <div class="card-header">
                      <h3 class="card-title">issue list history</h3>
                    </div>
                    <!-- /.card-header -->
                    <div class="card-body">
                        <table id="tbl_list_issue" class="table table-bordered table-striped dataTable" role="grid" aria-describedby="example1_info">
                        <thead>
                        <tr>
                            <th>category</th>
                            <th>book</th>
                            <th>userinfo</th>
                            <th>totaldays</th>
                            
                        </tr>
                        </thead>
                        <tbody>
                          <% issueList.forEach ((data)=>{ %>
                            <tr>
                               <td>
                                <%= data.Category.name %>
                               </td>
                               <td>
                                <%= data.Book.name %>
                               </td>
                               <td>
                                Name:<%= data.User.name %>
                                <br>
                                Email:<%= data.User.email %>
                               </td>
                               <td>
                                <%= data.days_issued %>
                               </td>
                               <td>
                                <%= data.issued_date %>
                               </td>
                            </tr>
                         <% }) %>
                        </tbody>
                        <tfoot>
                            <tr>
                                <th>category</th>
                                <th>book</th>
                                <th>userinfo</th>
                                <th>totaldays</th>
                               
                            </tr>
                        </tfoot>
                      </table>
